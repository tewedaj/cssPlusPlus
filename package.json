{
	"name": "inlineCssChanger",
	"displayName": "CSS++",
	"description": "Make writing React Native css a better experiance",
	"version": "0.1.19",
	"icon": "nmnm.png",
	"engines": {
		"vscode": "^1.64.2"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tewedaj/cssPlusPlus"
	},
	"publisher": "Tewedajgetahun",
	"activationEvents": [
		"onCommand:inlineCssChanger.helloWorld",
		"onCommand:inlineCssChanger.CssUpdate",
		"onCommand:inlineCssChanger.inlineCssChangerUpdateNoName",
		"onCommand:inlineCssChanger.inlineCssChangerExternalUpdate",
		"onCommand:inlineCssChanger.reactJsNoNameChanger",
		"onCommand:inlineCssChanger.reactJsWithName",
		"onCommand:inlineCssChanger.reactJsUpdateExternal",
		"onCommand:inlineCssChanger.htmlRandom",
		"onCommand:inlineCssChanger.htmlUpdateExternal",
		"onCommand:inlineCssChanger.htmlWithName",
		"onCommand:inlineCssChanger.AngularRandom",
		"onCommand:inlineCssChanger.AngularWithName",
		"onCommand:inlineCssChanger.AngularUpdate"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "inlineCssChanger.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "inlineCssChanger.CssUpdate",
				"title": "CssPlus: React-Native[Name]"
			},
			{
				"command": "inlineCssChanger.inlineCssChangerUpdateNoName",
				"title": "CssPlus: React-Native[Random]"
			},
			{
				"command": "inlineCssChanger.inlineCssChangerExternalUpdate",
				"title": "CssPlus: React-Native[update]"
			},
			{
				"command": "inlineCssChanger.reactJsNoNameChanger",
				"title": "CssPlus: React-JS[Random]"
			},
			{
				"command": "inlineCssChanger.reactJsWithName",
				"title": "CssPlus: React-JS[Name]"
			},
			{
				"command": "inlineCssChanger.reactJsUpdateExternal",
				"title": "CssPlus: React-JS[Update]"
			},
			{
				"command": "inlineCssChanger.htmlRandom",
				"title": "CssPlus: Html[Random]"
			},
			{
				"command": "inlineCssChanger.htmlUpdateExternal",
				"title": "CssPlus: Html[Update]"
			},
			{
				"command": "inlineCssChanger.htmlWithName",
				"title": "CssPlus: Html[Name]"
			},
			{
				"command": "inlineCssChanger.AngularRandom",
				"title": "CssPlus: Angular[Random]"
			},
			{
				"command": "inlineCssChanger.AngularWithName",
				"title": "CssPlus: Angular[Name]"
			},
			{
				"command": "inlineCssChanger.AngularUpdate",
				"title": "CssPlus: Angular[Update]"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.2",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
