{
	"name": "reactNativeCss",
	"displayName": "react-native-css",
	"description": "Make writing React Native css a better experiance",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:reactNativeCss.helloWorld",
		"onCommand:reactNativeCss.CssUpdate",
		"onCommand:reactNativeCss.ReactNativeCssUpdateNoName",
		"onCommand:reactNativeCss.ReactNativeCssExternalUpdate"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "reactNativeCss.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "reactNativeCss.CssUpdate",
				"title": "CssInline-React Native[N]"
			},
			{
				"command": "reactNativeCss.ReactNativeCssUpdateNoName",
				"title": "CssInline-React Native[R]"
			},
			{
				"command": "reactNativeCss.ReactNativeCssExternalUpdate",
				"title": "CssExternal-React Native"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}